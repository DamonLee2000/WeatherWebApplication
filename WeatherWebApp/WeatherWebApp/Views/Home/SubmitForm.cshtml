@{
}

@model SubmitFormViewModel
<html>
<head>
    <title>@Model.City Weather</title>
    <link rel="stylesheet" href="~/css/styles.css">
</head>
<body>
    <h1>@Model.City</h1>
    <!-- Other content goes here -->

    <p>@Model.Zipcode, @Model.Country</p>
    <p>@Model.Latitude, @Model.Longtitude</p>
    <p>@Model.TemperatureFahrenheit F</p>
    <p>@Model.TemperatureCelsius C</p>
    <p>@(Model.Humidity)% Humidity</p>
    <p>@Model.Weather</p>
    <p>@Model.Description</p>

    <img src="@string.Format("https://openweathermap.org/img/wn/{0}@2x.png", Model.icon)">

    
    

    <!-- Other content goes here -->
</body>
</html>



